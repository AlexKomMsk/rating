{
    "name": "rennokki/rating",
    "description": "Laravel Eloquent Rating allows you to assign ratings to any model.",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "rating",
        "rate",
        "stars",
        "star",
        "system"
    ],
    "homepage": "https://github.com/renoki-co/rating",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "alex@renoki.org",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^6.18.28|^7.22.1|^8.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^6.0",
        "orchestra/database": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Rating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Rating\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Rating\\RatingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
